<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="LocationMatch.Cloud" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="LocationMatch.Analysis.WorkerRole" vmsize="Small">
    <Imports></Imports>
  </WorkerRole>
  <WebRole name="LocationMatch.WebSite.WebRole" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="8080" />
    </Endpoints>
    <Imports></Imports>
  </WebRole>
</ServiceDefinition>